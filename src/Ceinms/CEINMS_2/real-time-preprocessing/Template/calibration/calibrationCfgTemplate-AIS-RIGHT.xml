<calibration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="calibration.xsd">

  <algorithm>
      <simulatedAnnealing>
      <noEpsilon>4</noEpsilon>
      <rt>0.1</rt>
      <T>200000</T>
      <NS>15</NS>
      <NT>5</NT>
      <epsilon>1.E-3</epsilon>
      <maxNoEval>2000000</maxNoEval>
    </simulatedAnnealing>
  </algorithm>

  <NMSmodel>
    <type>
      <openLoop />
    </type>

    <tendon>
      <equilibriumElastic />
    </tendon>

    <activation>
      <exponential />
    </activation>
  </NMSmodel>

    <calibrationSteps>
        <step>
            <dofs>knee_angle_r ankle_angle_r subtalar_angle_r</dofs> 
            <objectiveFunction>
               <minimizeTorqueError/>       
            </objectiveFunction>
            <parameterSet>
	            <parameter>
	                <name>c1</name>
                    <single />
                    <absolute>
                        <range>-0.95 -0.05</range>
                    </absolute>
	            </parameter>
	            <parameter>
	                <name>c2</name>
                    <single />
                    <absolute>
                        <range>-0.95 -0.05</range>
                    </absolute>
	            </parameter>
	            <parameter>
	                <name>shapeFactor</name>
                    <single />
                    <absolute>
                        <range>-2.999 -0.001</range>
                    </absolute>
	            </parameter>
	            <parameter>
	                <name>tendonSlackLength</name>
                    <single />
                    <relativeToSubjectValue>
                        <range>0.85 1.15</range>
                    </relativeToSubjectValue>
	            </parameter>
                <parameter>
	                <name>optimalFibreLength</name>
                    <single />
                    <relativeToSubjectValue>
                        <range>0.85 1.15</range>
                    </relativeToSubjectValue>
	            </parameter>
                <parameter>
                    <name>strengthCoefficient</name>
                    <muscleGroups>
                        <muscles>bifemlh_r bifemsh_r semimem_r semiten_r sar_r</muscles>                                    
                        <muscles>grac_r rect_fem_r tfl_r</muscles>                                
                        <muscles>per_brev_r per_long_r per_tert_r tib_ant_r</muscles>       
						<muscles>ext_dig_r ext_hal_r</muscles>
                        <muscles>flex_dig_r flex_hal_r</muscles>
                        <muscles>vas_int_r vas_lat_r vas_med_r</muscles>                         
                    </muscleGroups>
                    <absolute>
                        <range>1 3.5</range>
                    </absolute>
                </parameter>
                <parameter>
                    <name>strengthCoefficient</name>
                    <muscleGroups>
                        <muscles>lat_gas_r med_gas_r soleus_r</muscles>                                                           
                    </muscleGroups>
                    <absolute>
                        <range>0.5 1.5</range>
                    </absolute>
                </parameter>
            </parameterSet>
        </step> 
    </calibrationSteps>

  <trialSet></trialSet>
</calibration>